umount rozofs
cp /home/monitor/export.conf.clean /zbx/etc/rozofs/export.conf
cp /home/monitor/storage.conf.clean /zbx/etc/rozofs/storage.conf

python /home/monitor/store.pyc del


/etc/init.d/zoofs-exportd reload
sleep 3
/etc/init.d/zoofs-storaged reload
sleep 3
/etc/init.d/zoofs-manager-agent restart

